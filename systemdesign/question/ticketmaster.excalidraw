    {
      "id": "tcmoTWR9gscyoajQr-t-Q",
      "type": "text",
      "x": 1190.594363597037,
      "y": 4242.940702620021,
      "width": 794.1795043945312,
      "height": 175,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "#ffffff",
      "fillStyle": "solid",
      "strokeWidth": 2,
      "strokeStyle": "solid",
      "roughness": 1,
