    {
      "id": "LCixs08Z4goATod-90xy0",
      "type": "text",
      "x": 220.67648903519603,
      "y": 3080.851060169165,
      "width": 1581.3388671875,
      "height": 650,
      "angle": 0,
      "strokeColor": "#1e1e1e",
      "backgroundColor": "transparent",
      "fillStyle": "solid",
